{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "ResBody",
  "type": "object",
  "required": [
    "contests"
  ],
  "properties": {
    "contests": {
      "type": "array",
      "items": {
        "$ref": "#/definitions/Contest_for_Unknown"
      }
    }
  },
  "definitions": {
    "ContestId": {
      "type": "string",
      "format": "uuid"
    },
    "ContestStatus": {
      "enum": [
        "Upcoming",
        "Open",
        "Closed",
        "Archived"
      ]
    },
    "Contest_for_Unknown": {
      "type": "object",
      "required": [
        "category",
        "id",
        "polls",
        "status",
        "title"
      ],
      "properties": {
        "category": {
          "type": "string"
        },
        "event_start_at": {
          "type": [
            "string",
            "null"
          ],
          "format": "date-time"
        },
        "id": {
          "$ref": "#/definitions/ContestId"
        },
        "polls": {
          "$ref": "#/definitions/Unknown"
        },
        "status": {
          "$ref": "#/definitions/ContestStatus"
        },
        "title": {
          "type": "string"
        }
      }
    },
    "Unknown": {
      "type": "null"
    }
  }
}